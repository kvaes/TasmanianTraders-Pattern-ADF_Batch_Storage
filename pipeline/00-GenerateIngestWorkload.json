{
	"name": "00-GenerateIngestWorkload",
	"properties": {
		"activities": [
			{
				"name": "Wait1",
				"type": "Wait",
				"typeProperties": {
					"waitTimeInSeconds": 1
				}
			},
			{
				"name": "Get Files from Sample Workload",
				"type": "GetMetadata",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"dataset": {
						"referenceName": "AzureBlob_adftest_input",
						"type": "DatasetReference"
					}
				}
			}
		],
		"variables": {
			"VarStorageStagingContainer": {
				"type": "String",
				"defaultValue": "input"
			},
			"VarStorageProcessedContainer": {
				"type": "String",
				"defaultValue": "processed"
			},
			"VarStorageSampleContainer": {
				"type": "String",
				"defaultValue": "samples"
			}
		}
	}
}