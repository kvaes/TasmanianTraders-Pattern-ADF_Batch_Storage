{
	"name": "AzureBlob_adftest_file",
	"properties": {
		"parameters": {
			"InputFolder": {
				"type": "string",
				"defaultValue": "samples"
			},
			"InputFile": {
				"type": "string",
				"defaultValue": "sample.done"
			}
		},
		"type": "AzureBlob",
		"typeProperties": {
			"fileName": {
				"value": "@dataset().InputFile",
				"type": "Expression"
			},
			"folderPath": {
				"value": "@dataset().InputFolder",
				"type": "Expression"
			}
		}
	}
}